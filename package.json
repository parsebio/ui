{
  "name": "ui",
  "version": "0.0.0",
  "description": "Trailmaker App UI",
  "main": "server.js",
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=development node --max-http-header-size=81920 server.js",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "prod": "NODE_ENV=production node --max-http-header-size=81920 server.js",
    "pretest": "",
    "test": "jest --silent",
    "coverage": "jest --coverage --silent",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "test:fast": "jest --silent",
    "lint": "eslint ./src",
    "detect-secrets": "test -d .ds-venv || python3 -m venv .ds-venv; ./.ds-venv/bin/pip -q install detect-secrets; git diff -z --staged --name-only | xargs -0 ./.ds-venv/bin/detect-secrets-hook --baseline .secrets.baseline",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "pre-commit": [
    "lint",
    "detect-secrets"
  ],
  "keywords": [
    "nextjs",
    "antd",
    "react",
    "ssr",
    "redux",
    "redux-thunk"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
